<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/users.css">
</head>

<body>
    <header class="main-header"></header>
    <main class="main-main">
        <% if (users.length > 0) { %>
            <ul>
                <% for (let user of users) { %>
                    <li> <%= user.name %></li>
                <% } %>
            </ul>
        <% } else { %>
            <h1>There are no registered users!</h1>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>
